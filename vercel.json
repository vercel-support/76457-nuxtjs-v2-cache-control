{
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {}
    }
  ],
  "headers": [
    {
      "source": "/",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "s-maxage=86400, stale-while-revalidate"
        }
      ]
    }
  ]
}
